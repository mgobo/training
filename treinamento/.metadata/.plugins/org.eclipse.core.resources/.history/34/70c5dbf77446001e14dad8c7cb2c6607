@Component
public class FisicaMapperImpl implements FisicaMapper {

    @Override
    public Fisica toEntity(FisicaDto fisicaDto) {
        if ( fisicaDto == null ) {
            return null;
        }

        Fisica fisica = new Fisica();

        fisica.setId( fisicaDto.getId() );

        return fisica;
    }

    @Override
    public FisicaDto toDto(Fisica fisica) {
        if ( fisica == null ) {
            return null;
        }

        FisicaDto fisicaDto = new FisicaDto();

        fisicaDto.setId( fisica.getId() );

        return fisicaDto;
    }
}
