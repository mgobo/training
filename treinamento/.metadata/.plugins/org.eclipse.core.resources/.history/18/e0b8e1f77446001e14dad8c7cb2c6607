@Component
public class JuridicaMapperImpl implements JuridicaMapper {

    @Override
    public Juridica toEntity(JuridicaDto juridicaDto) {
        if ( juridicaDto == null ) {
            return null;
        }

        Juridica juridica = new Juridica();

        juridica.setId( juridicaDto.getId() );
        juridica.setCnpj( juridicaDto.getCnpj() );

        return juridica;
    }

    @Override
    public JuridicaDto toDto(Juridica juridica) {
        if ( juridica == null ) {
            return null;
        }

        JuridicaDto juridicaDto = new JuridicaDto();

        juridicaDto.setId( juridica.getId() );
        juridicaDto.setCnpj( juridica.getCnpj() );

        return juridicaDto;
    }
}
